USE stringFib;
//*See <a href="http://rosettacode.org/wiki/Benford%27s_law">Benford's law on Rosetta Code</a>
main->printf('%6d %8.2f',
             'actual'|[1..1000].each(i,softCast(copy(stringFib(i),0,1))).elementFrequency.sort({$x.get(1)<=$y.get(1)}).getInner(0),
             'expected'|(ln(1+1/[1..9])/ln(10)*1000));
