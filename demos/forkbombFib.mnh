private forkbombFib(i:int)->i<1 ? 1 : [i-1,i-2].pEach(arg,executor.exec([myPath,arg]).head.softCast,+);
//*Returns the i-th Fibonacci number using forked calls of the MNH-Interpreter
main(i)->print(forkbombFib(softCast(i)));
//*Returns the 1st through 9th Fibonacci number
main->[1..9].each(i,main(i));
