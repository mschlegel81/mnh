//@Algorithm Name=Euklidean Algorithm, Greatest common divider
gcd(a)->a;
gcd(a,0)->a;
gcd(a,b)->gcd(b, a mod b);
gcd(a,b,...)->gcd@(gcd(a,b)|...);

