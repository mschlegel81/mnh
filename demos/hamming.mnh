private H(nMax)->unique(flatten(each(i,2^[0..floor(ln(nMax      )/ln(2))],
                        flatten(each(j,3^[0..floor(ln(nMax/ i   )/ln(3))],
                                       5^[0..floor(ln(nMax/(i*j))/ln(5))]*i*j)))));
private H:=H(2^31);
//*See <a href="http://rosettacode.org/wiki/Hamming_numbers">http://rosettacode.org/wiki/Hamming_numbers</a>
main->print('The first 20 Hamming numbers are: ',head(H,20))|
      print('The 1691st Hamming number is    : ',H%1690);
