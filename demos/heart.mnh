h(x)->[12*sin(x)-4*sin(3*x),
       13*cos(x)-5*cos(2*x)-2*cos(3*x)-cos(4*x)-4];

main->begin
  setOptions([['axisStyleX',0],['axisStyleY',0],['autoscaleFactor',0.95]]);
  plot(([0..1000]/1000*2*pi).each(x,h(x)),'red i l 2');
  renderToFile(replace(myPath&"#","mnh#","png"),768,768,3);
end;

