private memoized hc(k<=2):=1;
private          hc(k)->hc(hc(k-1))+hc(k-hc(k-1));

private maxBetween(i0,i1)->maxBetween(i0,i1,each(i,[i0..i1],hc(i)/i));
private maxBetween(i0,i1,L)->max(L)&" @i="&(i0+argMax(L));

//*This solves the <a href="http://rosettacode.org/wiki/Hofstadter-Conway_$10,000_sequence"> Hofstadter-Conway $10,000 sequence task from Rosetta Code</a>
main->each(i,[0..19],and,print("range 2^",i,'..2^',i+1,' max. a(n)/n=',maxBetween(2^i,2^(i+1)))) and
      print("Mallows Number: ",max([1..2^20]%each(i,[1..2^20],hc(i)/i>0.55)));
