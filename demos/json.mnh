USE jsonUtils;

json:=//!~{
  "Herausgeber": "Xema",
  "Nummer": "1234-5678-9012-3456",
  "Deckung": 2e+6,
  "Waehrung": "EURO",
  "Inhaber":
  {
    "Name": "Mustermann",
    "Vorname": "Max",
    "maennlich": true,
    "Hobbys": [ "Reiten", "Golfen", "Lesen"],
    "Alter": 42,
    "Kinder": [],
    "Partner": null
  }
}~;

main->begin
  local struct:=json.parseJson('-');
  print(struct['Inhaber','Hobbys']);
  print(struct['Deckung']);
  print(struct.each(e,index<3?e:void).toMap.formatJson);
end;
