private coprimesLesser(n)->begin
  local result:=[2..n-1];
  factorize(n).unique.each(factor,
    result:=result[result mod factor!=0]);
  result;
end;

private curry isKnoedel(n,m)->!isPrime(m) AND coprimesLesser(m).pEach(i,powMod(i,m-n,m)=1,AND);

