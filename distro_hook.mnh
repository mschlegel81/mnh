USE packCode;
main->begin
  ['mnh.exe',
   'mnh_light.exe']
   .filter(::fileExists)
   .map({[$f,'C:\bin\'&extractFileName($f)]})
   .map({begin
      deleteFile($f[1]);
      copyFile@$f
      ? print('Copied ',$f[0],' -> C:\bin\',$f[1])
      : fail('Copy ',$f[0],' -> C:\bin\',$f[1],' failed');
    end});
  packCode.main;
end;
