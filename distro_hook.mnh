main->begin
  ['mnh.exe',
   'mnh_light.exe',
   'mnhi.exe',
   '..\art.bin\workflowScriptTemplate.mnh']
   .filter(::fileExists)
   .map({[$f,'C:\bin\'&extractFileName($f)]})
   .map({begin
      deleteFile($f[1]);
      copyFile@$f
      ? print('Copied ',$f[0],' -> ',$f[1])
      : fail('Copy ',$f[0],' -> ',$f[1],' failed');
    end});
end;
