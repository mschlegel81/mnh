//Pseudo-equality
each(i,[1..3],i);
//Simple aggregation without body
each(i,[1..3],+);
//Complex aggregation without body
each(i,[1..3],&" "&);
//List aggregation with body
each(i,[1..3],1/i);
//Simple aggregation with body
each(i,[1..3],+,1/i);
//Complex aggregation with body
each(i,[1..3],&" "&,1/i);

