writeFileLines('install.vbs',
['Set UAC = CreateObject("Shell.Application")',
 'UAC.ShellExecute "cmd", "/c assoc .mnh=mnh.script && ftype mnh.script=# %1 %*", "", "runas", 1'
].replace('#',executor));
writeFileLines('uninstall.vbs',
['Set UAC = CreateObject("Shell.Application")',
 'UAC.ShellExecute "cmd", "/c ftype mnh.script= && assoc .mnh= && rmdir /S /Q %ALLUSERSPROFILE%\MNH_IMIG && del #",  "", "runas", 1'
].replace('#',executor));
