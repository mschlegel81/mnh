writeFileLines('install.vbs',
['Set UAC = CreateObject("Shell.Application")',
 'UAC.ShellExecute "cmd", "/c assoc .mnh=mnh.script assoc .mnhwf=mnhwf.script && ftype mnh.script=# %1 %* && ftype mnhwf.script=# %1 %*", "", "runas", 1'
].replace('#',executor));
writeFileLines('uninstall.vbs',
['Set UAC = CreateObject("Shell.Application")',
 'UAC.ShellExecute "cmd", "/c ftype mnh.script= && ftype mnhwf.script= && assoc .mnh= && assoc .mnhwf= && rmdir /S /Q %ALLUSERSPROFILE%\MNH_IMIG && del #",  "", "runas", 1'
].replace('#',executor));
